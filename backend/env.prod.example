# 生产环境配置示例
APP_ENV=prod
DEBUG=false
DATABASE_URL=sqlite:///./data/personalfinance.db
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173"]
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log
FUND_API_TIMEOUT=10
FUND_API_RETRY_TIMES=3

# OKX API配置
OKX_API_KEY=你的APIKey
OKX_SECRET_KEY=你的SecretKey
OKX_PASSPHRASE=你的Passphrase
OKX_SANDBOX=false 

# Web3 API配置
WEB3_API_KEY=你的Web3_APIKey
WEB3_API_SECRET=你的Web3_SecretKey
WEB3_PROJECT_ID=你的Web3_ProjectId
WEB3_ACCOUNT_ID=你的Web3_AccountId
WEB3_PASSPHRASE=你的Web3_Passphrase 