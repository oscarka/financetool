# DeepSeek AI API配置示例
# 复制此文件为 .env 并填入你的实际配置

# DeepSeek AI API配置
DEEPSEEK_API_KEY=你的DeepSeek_API密钥
DEEPSEEK_API_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_MAX_TOKENS=4000
DEEPSEEK_TEMPERATURE=0.7

# 数据库配置
DATABASE_URL=postgresql://financetool_user:financetool_pass@localhost:5432/financetool_test
DATABASE_PERSISTENT_PATH=./data

# 其他配置
APP_ENV=test
DEBUG=true
LOG_LEVEL=INFO
